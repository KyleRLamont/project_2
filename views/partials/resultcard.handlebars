{{#each searchResult}}
<div class="col-sm-6">
  <div class="card">
    <div class="card-body">
      <h5 class="card-title" id="airline">{{airline}}</h5>
      <p class="card-text"><span id="departureCity">{{departure_city}}</span> - <span id="destination">{{destination}}</span>
      </p>
      <p class="card-text">Departure Date: <span id="departureDate">{{departure_at}}</span>
      </p>
      <p class="card-text">Departure Time: <span id="departureTime">#departureTime</span>
      </p>
      <a href="#" class="btn btn-primary savebtn">#savebtn</a>
    </div>
  </div>
</div>
{{/each}}